{
    "manifest_version": 3,
    "name": "DigiSploit",
    "description": "DigiSploit, Noordhoff exploit gemaakt door github.com/mnjake",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["main.js"],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources":["*.js", "*.css"],
            "matches":["<all_urls>"]
        }
    ],
    "action": {
        "default_popup": "popup/popup.html"
    }
}